schema {
    query: Query
}

extend type Query {
    # all employees
    employees: [Employee!]!
    # employee by id
    employee(id: ID!): Employee
}

# an employee
type Employee {
    id: ID!
    firstName: String!
    lastName: String!
    age: Int!
    gender: Gender!
    projectId: ID
    # the project this employee is currently assigned to
    project: Project
}

enum Gender {
    FEMALE,
    MALE,

}

